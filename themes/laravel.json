{
    "$schema": "vscode://schemas/color-theme",
    "name": "Laravel",
    "type": "light",
    "colors": {
        "editor.foreground": "#090910",
        "editor.background": "#fbfbfb",
        "editorIndentGuide.background": "#e6e6e6",
        "editorLineNumber.foreground": "#aaaaaa",
        "editorLineNumber.activeForeground": "#808080",
        "editor.selectionBackground": "#E7E8F2",
        "editor.inactiveSelectionBackground": "#E7E8F2",
        "editor.selectionHighlightBackground": "#E7E8F2",

        "activityBar.background": "#fbfbfb",
        "activityBar.foreground": "#090910",
        "sideBarSectionHeader.background": "#E3F1F6",
        "sideBar.background": "#fbfbfb",
        "sideBar.border": "#e6e6e6",
        "list.activeSelectionBackground": "#055472",
        "list.focusOutline":"#055472",

        "activityBarBadge.background": "#E3F1F6",
        "activityBarBadge.foreground": "#090909",
        "statusBar.background": "#fbfbfb",
        "statusBar.foreground": "#090910",
        "statusBar.debuggingBackground": "#ca473f"
    },
    "tokenColors": [
        {
            "name": "Comment, keyword documentation",
            "scope": [
                "comment",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#93939e"
            }
        },
        {
            "name": "Variables, puctuation",
            "scope": [
                "string constant.other.placeholder",
                "punctuation",
                "meta.function-call.arguments",
                "constant.character",
                "variable."
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#981d15"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#055472"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "constant.other.color",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method",
                "entity.other.inherited-class",
            ],
            "settings": {
                "foreground": "#ca473f"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.escape"
            ],
            "settings": {
                "foreground": "#981d15"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "markup.heading",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#690"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#ca473f"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#ca473f"
            }
        },
        {
            "name": "this, self",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#0782b1"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#0782b1"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#0782b1"
            }
        },

        {
            "name": "Python: Function Builtin",
            "scope": [
                "support.function.builtin.python"
            ],
            "settings": {
                "foreground": "#0782b1"
            }
        },
        {
            "name": "Python: Decorator",
            "scope": [
                "entity.name.function.decorator.python",
                "punctuation.definition.decorator.python"
            ],
            "settings": {
                "foreground": "#0782b1"
            }
        },
        {
            "name": "Javascript:",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "Javascript: string in jsx",
            "scope": [
                "meta.jsx.children.js string"
            ],
            "settings": {
                "foreground": "#055472"
            }
        },
        {
            "name": "PHP: variable",
            "scope": [
                "variable.language.this.php punctuation.definition.variable.php",
                "punctuation.definition.variable.php",
                "variable.other.php"
            ],
            "settings": {
                "foreground": "#0782B1"
            }
        },
        {
            "name": "PHP: class use",
            "scope": [
                "keyword.operator.class.php",
                "support.class.php",
                "support.other.namespace.php",
                "punctuation.separator.inheritance.php"
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "PHP: Type in documentation",
            "scope": [
                "comment.block.documentation.phpdoc.php support.class.php",
                "comment.block.documentation.phpdoc.php support.other.namespace.php",
                "comment.block.documentation.phpdoc.php punctuation.separator.inheritance.php",
                "comment.block.documentation.phpdoc.php punctuation.separator.delimiter.php",
                "comment.block.documentation.phpdoc.php keyword.other",
                "comment.block.documentation.phpdoc.php support.class.builtin.php",
            ],
            "settings": {
                "foreground": "#93939E"
            }
        },

        {
            "name": "HTML: Name Tag",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#981d15"
            }
        },
        {
            "name": "HTML: punctuation tag",
            "scope": [
                "punctuation.definition.tag",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "HTML: attribute",
            "scope": [
                "meta.tag"
            ],
            "settings": {
                "foreground": "#690"
            }
        },
        {
            "name": "HTML: String",
            "scope": [
                "string.quoted.double.html",
                "punctuation.definition.string.end.html",
                "punctuation.definition.string.begin.html"
            ],
            "settings": {
                "foreground": "#055472"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "CSS Classes",
            "scope": [
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#055472"
            }
        },
        {
            "name": "CSS punctuation definition",
            "scope": [
                "source.css keyword.other.unit"
            ],
            "settings": {
                "foreground": "#981D15"
            }
        },
        {
            "name": "JSON: Puntuation",
            "scope": [
                "support.type.property-name.json",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json"
            ],
            "settings": {
                "foreground": "#055472"
            }
        },

        {
            "name": "Django",
            "scope": [
                "storage.type.templatetag.django"
            ],
            "settings": {
                "foreground": "#ca473f"
            }
        },
        {
            "name": "Django",
            "scope": [
                "storage.type.variable.django",
            ],
            "settings": {
                "foreground": "#090910"
            }
        },
        {
            "name": "Django",
            "scope": [
                "string.unquoted.tag-string.django"
            ],
            "settings": {
                "foreground": "#0782B1"
            }
        },
        {
            "name": "Blade",
            "scope": [
                "keyword.blade",
                "support.function.construct.begin.blade",
                "support.function.construct.end.blade"
            ],
            "settings": {
                "foreground": "#ca473f"
            }
        },
        {
            "name": "Blade",
            "scope": [
                "support.function.construct.begin.blade",
                "support.function.construct.end.blade"
            ],
            "settings": {
                "foreground": "#090910"
            }
        }
    ]
}
